{
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://localhost:5050"
      }
    }
  },
  "ReverseProxy": {
    "Routes": {
      "route1": {
        "ClusterId": "backend",
        "Match": {
          "Path": "{**catch-all}"
        }
      }
    },
    "Clusters": {
    "backend": {
      "Destinations": {
        "backend1": {
          "Address": "http://localhost:3000/"
        }
      },
      "HttpRequest": {
        "Timeout": "00:05:00"  // 5 minutes
      }
    }
  }
  }
}
